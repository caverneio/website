{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "content": ["./.contentlayer/generated"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.mdx",
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "next.config.js",
    ".contentlayer/**/*"
, "src/lib/contentlayer/style-code-component/components/markup.ssx", "src/lib/contentlayer/style-code-component.jsx"  ],
  "exclude": ["node_modules"]
}
